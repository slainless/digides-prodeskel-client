{
  "name": "prodeskel-ws",
  "type": "module",
  "author": "Aiman Fauzy",
  "description": "Basic WebSocket client implementations for DIGIDES Prodeskel Service.",
  "version": "0.1.1",
  "repository": {
    "type": "git",
    "url": "https://github.com/slainless/prodeskel-ws.git"
  },
  "devDependencies": {
    "@ryoppippi/unplugin-typia": "npm:@jsr/ryoppippi__unplugin-typia",
    "@types/bun": "^1.1.6",
    "@types/node": "^20.14.9",
    "esbuild": "^0.22.0",
    "ts-patch": "latest",
    "typescript": "5.4.2"
  },
  "dependencies": {
    "typia": "^6.3.1"
  },
  "scripts": {
    "build:browser": "rm -rf dist && tsc -p ./src/browser/tsconfig.json && bun ./scripts/build.browser.ts",
    "prepare": "ts-patch install && typia patch"
  },
  "exports": {
    "./browser": {
      "require": "./dist/browser/index.cjs",
      "import": "./dist/browser/index.mjs",
      "default": "./dist/browser/index.js",
      "types": "./dist/browser/index.d.ts"
    },
    "./schema": {
      "types": "./dist/schema/index.d.ts"
    },
    "./core": {
      "require": "./dist/core/index.cjs",
      "import": "./dist/core/index.mjs",
      "types": "./dist/core/index.d.ts"
    }
  },
  "license": "MIT"
}