{
  "$schema": "https://jsr.io/schema/config-file.v1.json",
  "name": "@slainless/prodeskel-ws",
  "version": "0.1.7",
  "description": "Basic WebSocket client implementations for DIGIDES Prodeskel Service.",
  "exports": {
    "./browser": "./dist/browser/index.d.ts",
    "./schema": "./dist/schema/index.d.ts",
    "./core": "./dist/core/index.d.ts"
  },
  "publish": {
    "include": [
      "dist/**/*.js",
      "dist/**/*.d.ts",
      "README.md",
      "package.json"
    ],
    "exclude": [
      "src",
      ".github",
      "!dist",
      "!dist/**/*.js",
      "!dist/**/*.d.ts"
    ]
  }
}